@model EngLine.Models.StudentResponse
<div style="background-color: #6b2dae; width: 100%; height: 93px;"></div>
<div class="card" style="max-width: 700px;box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);margin-left: auto;margin-right: auto;margin-bottom: 25px;margin-top: 25px;">
	<div class="card-header">
		Kết quả
	</div>
	<div class="card-body">
		<h5 class="card-title"></h5>
		<p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
		<dl class="row">
			<dt class="col-sm-3">Score</dt>
			<dd class="col-sm-9">@Html.DisplayFor(model => model.Score)</dd>

			<dt class="col-sm-3">Student ID</dt>
			<dd class="col-sm-9">@Html.DisplayFor(model => model.Student.Id)</dd>

			<dt class="col-sm-3">Test ID</dt>
			<dd class="col-sm-9">@Html.DisplayFor(model => model.Test.Id) <a href="#" class="btn btn-primary">Go somewhere</a></dd>
		</dl>
	</div>
</div>